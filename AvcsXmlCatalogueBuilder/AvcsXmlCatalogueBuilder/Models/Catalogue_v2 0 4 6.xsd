<?xml version="1.0" encoding="UTF-8"?>
<!-- edited with XMLSpy v2013 rel. 2 sp2 (http://www.altova.com) by Paula Ballands (UK Hydrographic Officer) -->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified" version="2.0.4.6" id="UKHO-CATALOGUE">
	<!--<xs:import namespace="http://www.w3.org/1999/xlink" schemaLocation="xlink.xsd"/>-->
	<xs:element name="UKHOCatalogueFile">
		<xs:annotation>
			<xs:documentation>
        Catalogue Version Control Change Log:
        Baselined as 2.0.0 ES 04\11\09
        Change 2.0.1 ES 24\11\09
        1) Polygon on global geographic limit made unbounded

        Change 2.0.2 (Verified as correct version SG 24/06/2011)
        1) New ACRS Folio type added to Unit type

        Change 2.0.3 SG 14/04/2011: To Support e-Navigator 2.4 and the Product Information Database
        1) UnitOfSale/Status type becomes TypeUnitStatus (instead of String)
        2) TypeChartStatus updated to reflect new PID states
        3) TypeUnitStatus added to describe new unit of sale PID states
        4) New ToBeNewEdition element added under ChartStatus
        5) New ToBeCancelled element added under ChartStatus
        6) New ReplacesList element added under ChartStatus, with Replaces attribute
        7) New ReplacedByList element added under ChartStatus, with ReplacedBy attribute

        Change 2.0.4 CHP 20130909
        1) Added support for eNP Catalogues
        2) Update enumerations UnitType and ProductTypeCode
        3) Added complex types SearchKeywords and PublicationsMetaData
        4) Added element ElectronicPublications to DigitalPublications

        Change 2.0.4.1 CHP 20130910
        1) Updated eNPMetadata to an extension of PublicationsMetadata

        Change 2.0.4.2 CHP 20131113
        1) Incorporated changes from correct live version 2.0.3
        2)Updated enumeration productTypeCode for new e-NP codes

        Change 2.0.4.3 CHP 20140205
        1) Updated unitType and productTypeCode enumerations for ADRS 2

        Change 2.0.4.4 20140731
        1) Added support for Additional Coverage scenarios to ChartStatus element

        Change 2.0.4.5 20141009
        1) Updated unitType and productTypeCode enumerations for ADRS 1345
        2) Added unit types for e-NP
		
		Change 2.0.4.6 20151112
		1) Added a AdmiraltyApplication element into the DigitalProduct Element
		2) Added a new Usage Code for Passage Planner

      </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="BaseFileMetadata">
					<xs:annotation>
						<xs:documentation>
              Metadata about the catalogue file  #1 IHO TRANSFER STANDARD for DIGITAL HYDROGRAPHIC DATA http://www.iho.shom.fr/publicat/free/files/31Main.pdf
              3.1
              November 2000
            </xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="MD_FileIdentifier" type="xs:string">
								<xs:annotation>
									<xs:documentation>Unique ID for this metadata file</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="MD_CharacterSet" type="xs:string">
								<xs:annotation>
									<xs:documentation>
                    full name of the ISO character coding standard used for documenting a plain text C / ISO/IEC 10646-1
                    [#1 B.2.14]
                  </xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="MD_PointOfContact">
								<xs:annotation>
									<xs:documentation>identification of, and means of communication with, person(s) and organization(s) associated with the resource(s) [#1 B.2.2.1.29]</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element ref="ResponsibleParty">
											<xs:annotation>
												<xs:documentation>identification of, and means of communication with, person(s) and organizations associated with the dataset (MD_Identification) [#1 B.3.2.2.374]</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="MD_DateStamp" type="xs:date">
								<xs:annotation>
									<xs:documentation>Date that the metadata was created</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="MD_StandardName" type="xs:string">
								<xs:annotation>
									<xs:documentation>name of the metadata standard including profile name</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="MD_StandardVersion" type="xs:string">
								<xs:annotation>
									<xs:documentation>Version of the metadata standard used</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Products">
					<xs:annotation>
						<xs:documentation>Product Catalogue Metadata</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Digital" minOccurs="0" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Digital products produced by UKHO</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="ENC" minOccurs="0" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Electronic Navigational Chart</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element ref="ShortName">
														<xs:annotation>
															<xs:documentation>Short name used for  fast searching</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="Metadata" type="S57Metadata">
														<xs:annotation>
															<xs:documentation>Product Metadata derived from base metadata</xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="ARCS" minOccurs="0" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Admiralty Raster Chart Service</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element ref="ShortName">
														<xs:annotation>
															<xs:documentation>Short name used for  fast searching</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="Metadata" type="ARCSMetadata"/>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="ATPL" minOccurs="0" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Admiralty Temporary and Preliminary Layer</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element ref="ShortName"/>
													<xs:element name="Metadata" type="S57Metadata"/>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="TotalTide" minOccurs="0" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Admiralty Total Tide</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element ref="ShortName"/>
													<xs:element name="Metadata" type="PublicationsMetadata"/>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element ref="ListOfLights" minOccurs="0" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Admiralty List of Lights</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element ref="RadioSignals" minOccurs="0" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Admiralty List of Radio Signals</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="ElectronicPublications" minOccurs="0" maxOccurs="unbounded">
											<xs:complexType>
												<xs:sequence>
													<xs:element ref="ShortName"/>
													<xs:element name="Metadata" type="eNPMetadata"/>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="AdmiraltyApplications" minOccurs="0" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Admiralty Application</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element ref="ShortName"/>
													<xs:element name="Metadata" type="PublicationsMetadata"/>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="Paper" minOccurs="0" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Paper products produced by UKHO</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="StandardNavigationChart" minOccurs="0" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Navigational Paper Chart Metadata</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element ref="ShortName"/>
													<xs:element name="Metadata" type="ARCSMetadata"/>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="ThematicChart" minOccurs="0" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Thematic Chart Metadata</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element ref="ShortName"/>
													<xs:element name="Metadata" type="ARCSMetadata"/>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="SailingDirections" minOccurs="0" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Details of the sailing directions books and volumes</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element ref="ShortName"/>
													<xs:element name="Metadata" type="PublicationsMetadata"/>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="TideTables" minOccurs="0" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Details of the Tide Tables books and volumes</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element ref="ShortName"/>
													<xs:element name="Metadata" type="PublicationsMetadata"/>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="TidalStreamAtlas" minOccurs="0" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Details of the Tidal Stream Atlas books and volumes</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element ref="ShortName"/>
													<xs:element name="Metadata" type="PublicationsMetadata"/>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element ref="ListOfLights" minOccurs="0" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Details of the List of Lights books and volumes</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element ref="RadioSignals" minOccurs="0" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Details of the Radio Signals books and volumes</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="NMPublications" minOccurs="0" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Details of the Notices to mariners publications</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element ref="ShortName"/>
													<xs:element name="Metadata" type="PublicationsMetadata"/>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element ref="MiscPublications" minOccurs="0" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Details of the books and volumes not included in sections above</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="UKHOCatalogue" minOccurs="0" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>TBC</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element ref="ShortName"/>
													<xs:element name="Metadata" type="PublicationsMetadata"/>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="UnitOfSale" minOccurs="0" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Unit of Sale contains the prices and high level definitions of all UKHO products in the catalogue XML file. The diagram to the left details of the unit types currently used in the catalogue. This section contains the sales hierarchy for all UKHO sellable products.</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element ref="ID">
											<xs:annotation>
												<xs:documentation>Unique folio ID </xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="SAPIPN" type="MinLenOne"/>
										<xs:element name="CatalogueNumber" type="MinLenOne"/>
										<xs:element name="Title" type="MinLenOne">
											<xs:annotation>
												<xs:documentation>Geographical title of the folio</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="Usage" type="productTypeCode">
											<xs:annotation>
												<xs:documentation>Usage Band or Scale</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="UnitType" type="UnitType">
											<xs:annotation>
												<xs:documentation>Use e.g. AVCS - Transit, Regional, Port. ARCS - World, Regional, Chart </xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element ref="GeographicLimit">
											<xs:annotation>
												<xs:documentation>Geographical extent of the Unit</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="Duration" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Available purchasable licensing periods</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element ref="Price"/>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="Status" type="TypeUnitStatus" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Current status of the folio e.g. live, deprecated etc.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="SubUnit" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Units contained in this unit (i.e cells in a unit, units in a folio)</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="Unit" maxOccurs="unbounded" type="xs:string" />
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element ref="Notes" minOccurs="0" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Additional textual or a reference to further information</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="Software" minOccurs="0" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Software patches, service packs and software release information  released by UKHO</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element ref="ID"/>
										<xs:element name="SoftwareType">
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:enumeration value="Hotfix"/>
													<xs:enumeration value="ServicePack"/>
													<xs:enumeration value="Release"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="ThirdPartyProduct" minOccurs="0" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Third party products suppiled by Third Party Distributors</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element ref="ID"/>
										<xs:element ref="ShortName"/>
										<xs:element name="Metadata">
											<xs:complexType>
												<xs:complexContent>
													<xs:extension base="ThirdPartyMetadata"/>
												</xs:complexContent>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="Agents" minOccurs="0" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>A listing of UKHO distributors and contact details</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="SAPID" type="MinLenOne">
											<xs:annotation>
												<xs:documentation>The SAP identifier for this agent</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="AgentDiscount" minOccurs="0" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>UKHO discount that this agent is allocated. This data is sensitive abd should not be publically available.</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="ProductType" type="productTypeCode">
														<xs:annotation>
															<xs:documentation>ARCS, AVCS, SNC and Pubs</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="Discount">
														<xs:annotation>
															<xs:documentation>% of total discount</xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element ref="ResponsibleParty">
											<xs:annotation>
												<xs:documentation>identification of, and means of communication with, person(s) and organizations associated with the dataset (MD_Identification)</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="AgentCategory">
											<xs:annotation>
												<xs:documentation>Category of distributor (Paper \ Digital \ Both \ None)</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:enumeration value="Paper"/>
													<xs:enumeration value="Digital"/>
													<xs:enumeration value="Both"/>
													<xs:enumeration value="None"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="PaperChartUpdatingAPI" type="PCU_API_Type" minOccurs="0" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>The paper chart updaing service provided by this distributor</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="SchemaVersion" type="xs:NMTOKEN" use="required" fixed="2.0.4.6">
				<xs:annotation>
					<xs:documentation>This enforces the use of the correct schema on validation</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="MetadataType">
		<xs:annotation>
			<xs:documentation>The generic metadata type form which all others inherit</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:annotation>
				<xs:documentation>See generic product metadata definition</xs:documentation>
			</xs:annotation>
			<xs:element ref="DatasetTitle">
				<xs:annotation>
					<xs:documentation>Title of the dataset</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="DatasetAbstract" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Abstract describing the data product</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="Scale" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Compilation Scale of the depiction of the real world objects in the dataset </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="GeographicLimit">
				<xs:annotation>
					<xs:documentation>Geographical Extent  of the information of dataset</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="TemporalExtent" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Time period covered by the content of the dataset</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="VerticalExtent" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Vertical extent of dataset</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="Lineage" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Information about the source data used in constructing the dataset</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="ReferenceSystem" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Description of the spatial and temporal reference systems used in the dataset</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="Notes" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Additional textual information or a reference to further textual information</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="Folio" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Folio(s) to which the product is included </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="SAP_IPN">
				<xs:annotation>
					<xs:documentation>SAP product code</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="CatalogueNumber">
				<xs:annotation>
					<xs:documentation>Catalogue product code</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Status" type="CTypeChartStatus" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Product availablity</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Unit" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Identifier(s) to product unit of sale</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="ID" type="xs:string"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="DatasetResponsibleParty">
		<xs:annotation>
			<xs:documentation>
        #1 IHO TRANSFER STANDARD for DIGITAL HYDROGRAPHIC DATA http://www.iho.shom.fr/publicat/free/files/31Main.pdf v3.1 November 2000
      </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="individualName" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>name of the responsible personsurname, given name, title separated by a delimiter  [#1 B.3.2.2.375]</xs:documentation>
